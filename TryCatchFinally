'use strict';

process.stdin.resume();
process.stdin.setEncoding('utf-8');

let inputString = '';
let currentLine = 0;

process.stdin.on('data', inputStdin => {
    inputString += inputStdin;
});

process.stdin.on('end', _ => {
    inputString = inputString.trim().split('\n').map(string => {
        return string.trim();
    });
    
    main();    
});

function readLine() {
    return inputString[currentLine++];
}

/*
 * Complete the reverseString function
 * Use console.log() to print to stdout.
 */
function reverseString(s) {
    //in try block: reverse string -> split, reverse, join methods
    let reversedString;
    let exceptionCaught = false;
    
    
    try{
        reversedString = s.split("").reverse().join("");
    }
    //catch block: catch exception and print exception msg in new line
    catch (e) {
        exceptionCaught = true;
        console.log(e.message);
    }
    finally{
        if (exceptionCaught){
            console.log(s);
        }
        else{
            console.log(reversedString);
        }
    }
    //print (reversed) s in new line if no exception was thrown. Print original s if exception was thrown.
    
}


function main() {
    const s = eval(readLine());
    
    reverseString(s);
}
